<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="400dp"
    android:layout_marginLeft="5dp"
    android:layout_marginTop="5dp"
    android:layout_marginRight="5dp"
    card_view:cardCornerRadius="5dp"
    card_view:cardElevation="10dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/nool_pid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:textSize="18sp"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_edit"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            tools:text="no data found..!" />

        <TextView
            android:id="@+id/nool_wp"
            android:layout_width="wrap_content"
            android:layout_height="15dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:text="WP"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_pid" />

        <TextView
            android:id="@+id/nool_wq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="96dp"
            android:layout_marginTop="32dp"
            android:text="WQ"
            card_view:layout_constraintStart_toEndOf="@+id/nool_wp"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_pid" />

        <TextView
            android:id="@+id/nool_tp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:text="TotalPrice"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_p"
            card_view:layout_constraintHorizontal_bias="0.555"
            card_view:layout_constraintStart_toEndOf="@+id/nool_date" />

        <TextView
            android:id="@+id/nool_cp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="20dp"
            android:text="CP"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_wp" />

        <TextView
            android:id="@+id/nool_cq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginTop="16dp"
            android:text="CQ"
            card_view:layout_constraintStart_toEndOf="@+id/nool_cp"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_wq" />

        <TextView
            android:id="@+id/nool_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="16dp"
            android:text="Paid"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_notpaid" />

        <TextView
            android:id="@+id/nool_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="16dp"
            android:text="Date"
            android:textSize="12sp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/nool_notpaid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="16dp"
            android:text="Not Paid"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/noolmatwp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="20dp"
            android:text="MatWP"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_cp" />

        <TextView
            android:id="@+id/noolmatwq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="68dp"
            android:layout_marginTop="20dp"
            android:text="MatWQ"
            card_view:layout_constraintStart_toEndOf="@+id/noolmatwp"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_cq" />

        <TextView
            android:id="@+id/noolmatcp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginTop="20dp"
            android:text="MatCP"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/noolmatwp" />

        <TextView
            android:id="@+id/noolmatcq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="76dp"
            android:layout_marginTop="20dp"
            android:text="MatCQ"
            card_view:layout_constraintStart_toEndOf="@+id/noolmatcp"
            card_view:layout_constraintTop_toBottomOf="@+id/noolmatwq" />

        <TextView
            android:id="@+id/nooltotalw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="20dp"
            android:text="totalw"
            card_view:layout_constraintBottom_toTopOf="@+id/nooltotalc"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.577"
            card_view:layout_constraintStart_toEndOf="@+id/nool_wq" />

        <TextView
            android:id="@+id/nooltotalc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="12dp"
            android:text="totalc"
            card_view:layout_constraintBottom_toTopOf="@+id/noolmattotalw"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.577"
            card_view:layout_constraintStart_toEndOf="@+id/nool_cq" />

        <TextView
            android:id="@+id/noolmattotalw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="12dp"
            android:text="mattotalw"
            card_view:layout_constraintBottom_toTopOf="@+id/noolmattotalc"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.507"
            card_view:layout_constraintStart_toEndOf="@+id/noolmatwq" />

        <TextView
            android:id="@+id/noolmattotalc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:text="mattotalc"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_m1"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toEndOf="@+id/noolmatcq" />

        <ImageView
            android:id="@+id/nool_del"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            card_view:layout_constraintBottom_toTopOf="@+id/nooltotalw"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.0"
            card_view:srcCompat="@android:drawable/btn_dialog" />

        <ImageView
            android:id="@+id/nool_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="8dp"
            card_view:layout_constraintBottom_toTopOf="@+id/nooltotalw"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_del"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.0"
            card_view:srcCompat="@android:drawable/ic_menu_edit" />

        <TextView
            android:id="@+id/nool_total1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="24dp"
            android:text="total1"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_total2"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/noolmatcp" />

        <TextView
            android:id="@+id/nool_total2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginBottom="16dp"
            android:text="total2"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_des"
            card_view:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/nool_ep1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="84dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="8dp"
            android:text="ep1"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_ep2"
            card_view:layout_constraintStart_toEndOf="@+id/nool_total1"
            card_view:layout_constraintTop_toBottomOf="@+id/noolmatcq"
            card_view:layout_constraintVertical_bias="0.047" />

        <TextView
            android:id="@+id/nool_ep2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="84dp"
            android:layout_marginBottom="16dp"
            android:text="ep2"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_des"
            card_view:layout_constraintStart_toEndOf="@+id/nool_total2" />

        <TextView
            android:id="@+id/nool_m1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="24dp"
            android:text="m1"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_m2"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.528"
            card_view:layout_constraintStart_toEndOf="@+id/nool_ep1" />

        <TextView
            android:id="@+id/nool_m2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="76dp"
            android:layout_marginBottom="16dp"
            android:text="m2"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_des"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.821"
            card_view:layout_constraintStart_toEndOf="@+id/nool_ep2" />

        <TextView
            android:id="@+id/nool_des"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:text="Description"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_tp"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toStartOf="parent" />
    </android.support.constraint.ConstraintLayout>

</android.support.v7.widget.CardView>