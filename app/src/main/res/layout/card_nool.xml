<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="220dp"
    android:layout_marginLeft="5dp"
    android:layout_marginTop="5dp"
    android:layout_marginRight="5dp"
    card_view:cardCornerRadius="5dp"
    card_view:cardElevation="10dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/nool_pid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:textSize="18sp"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_edit"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            tools:text="no data found..!" />

        <TextView
            android:id="@+id/nool_wp"
            android:layout_width="wrap_content"
            android:layout_height="15dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="32dp"
            android:layout_marginBottom="8dp"
            android:text="WP"
            android:textColor="#4CAF50"
            card_view:layout_constraintBottom_toTopOf="@+id/noolmatwp"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_pid"
            card_view:layout_constraintVertical_bias="0.044" />

        <TextView
            android:id="@+id/nool_wq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="WQ"
            android:textColor="#4CAF50"
            card_view:layout_constraintBottom_toTopOf="@+id/noolmatwq"
            card_view:layout_constraintEnd_toStartOf="@+id/nooltotalw"
            card_view:layout_constraintHorizontal_bias="0.475"
            card_view:layout_constraintStart_toEndOf="@+id/nool_wp"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_pid"
            card_view:layout_constraintVertical_bias="0.035" />

        <TextView
            android:id="@+id/nool_tp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:text="TotalPrice"
            android:textColor="@android:color/holo_red_dark"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_p"
            card_view:layout_constraintStart_toEndOf="@+id/nool_date" />

        <TextView
            android:id="@+id/nool_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="36dp"
            android:layout_marginBottom="16dp"
            android:text="Paid"
            android:textColor="#0429F8"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_notpaid" />

        <TextView
            android:id="@+id/nool_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="16dp"
            android:text="Date"
            android:textSize="12sp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/nool_notpaid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:text="Not Paid"
            android:textColor="#047708"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/noolmatwp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="16dp"
            android:text="MatWP"
            android:textColor="#4CAF50"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_total1"
            card_view:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/noolmatwq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:text="MatWQ"
            android:textColor="#4CAF50"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_ep1"
            card_view:layout_constraintEnd_toStartOf="@+id/noolmattotalw"
            card_view:layout_constraintHorizontal_bias="0.496"
            card_view:layout_constraintStart_toEndOf="@+id/noolmatwp" />

        <TextView
            android:id="@+id/nooltotalw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="totalw"
            android:textColor="#4CAF50"
            card_view:layout_constraintBottom_toTopOf="@+id/noolmattotalw"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/nool_edit"
            card_view:layout_constraintVertical_bias="0.088" />

        <TextView
            android:id="@+id/noolmattotalw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:text="mattotalw"
            android:textColor="#4CAF50"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_m1"
            card_view:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/nool_del"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            card_view:layout_constraintBottom_toTopOf="@+id/nooltotalw"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.0"
            card_view:srcCompat="@android:drawable/btn_dialog" />

        <ImageView
            android:id="@+id/nool_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="12dp"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_del"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:srcCompat="@android:drawable/ic_menu_edit" />

        <TextView
            android:id="@+id/nool_total1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="24dp"
            android:text="total1"
            android:textColor="#F44336"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_date"
            card_view:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/nool_ep1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="24dp"
            android:text="ep1"
            android:textColor="#F44336"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_tp"
            card_view:layout_constraintEnd_toStartOf="@+id/nool_m1"
            card_view:layout_constraintHorizontal_bias="0.417"
            card_view:layout_constraintStart_toEndOf="@+id/nool_total1" />

        <TextView
            android:id="@+id/nool_m1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="24dp"
            android:text="m1"
            android:textColor="#F44336"
            card_view:layout_constraintBottom_toTopOf="@+id/nool_notpaid"
            card_view:layout_constraintEnd_toEndOf="parent" />

    </android.support.constraint.ConstraintLayout>

</android.support.v7.widget.CardView>